<%= form_for(@category, url: admin_categories_path) do |f| %>
  <%= render "shared/form_error_messages", object: @category %>
  <%= f.label t("views.tag.name") %>
  <%= f.text_field :name %>
  <%= f.label t("views.tag.description") %>
  <%= f.text_field :description %>
  <%= f.submit t("views.tag.create"), class: "btn btn-primary" %>
<% end %>
